{
    "name": "normalcoder/easy-feishu",
    "description": "飞书 PHP SDK",
    "keywords": [
        "feishu",
        "larksuite",
        "feishu-sdk",
        "larksuite-sdk",
        "sdk"
    ],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "马运锋",
            "email": "yunfeng0614@qq.com",
            "role": "Developer"
        },
        {
            "name": "诺墨",
            "email": "normal@normalcoder.com",
            "homepage": "https://normalcoder.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-fileinfo": "*",
        "ext-mbstring": "*",
        "mayunfeng/supports": "^1.0",
        "pimple/pimple": "~3.0",
        "psr/simple-cache": "^3.0",
        "symfony/cache": "^3.3|^4.3|^5.2|^6.1",
        "symfony/http-foundation": "^2.7|^3.0|^4.0|^6.0",
        "symfony/psr-http-message-bridge": "~0.3|^2.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "EasyFeishu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EasyFeishu\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.3"
    }
}
